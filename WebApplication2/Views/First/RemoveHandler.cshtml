@model RemoveHandlerModel

@{
    ViewBag.Title = "RemoveHandler";
}

<h2>Do you want to remove this handler?</h2>

<input type="button" id="Ok" value="Ok" />
<input type="button" id="Cancel" value="Cancel" onclick='window.location="Configurations";' />
<div>
    @Html.ActionLink("Back to prevoius page", "Configurations")
</div>

<script>
    $("#Ok").click(function () {
        $.post("@Url.Action("handlerRemoved")", function () {
            window.location.href = "Configurations";
        });
    });
</script>
